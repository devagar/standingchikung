---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>mZero</title>
    <link>http://mZero.co</link>
    <atom:link href="http://mZero.co/rss.xml" rel="self" type="application/rss+xml" />
    <description>Ideias para simplificar</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>
 
    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://functionn.github.com{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>Lourenco de Azevedo</author>
      <guid>http://mZero.co{{ post.id }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}
 
  </channel>
</rss>