---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Chi Kung Stories</title>
  <link href="{{ base.url }}"/>
  <link type="application/atom+xml" rel="self" href={{ base.url }}/feed.atom"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ base.url }}</id>
  <author>
    <name>Lourenco de Azevedo</name>
    <email>lourencoazevedo@devagar.org</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>{{ base.url }}{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="{{ base.url }}{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>"Lourenco de Azevedo"</name>
      <uri>{{ base.url }}</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>